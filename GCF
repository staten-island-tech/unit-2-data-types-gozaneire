number1 = int(input("Insert a number"))
number2 = int(input("Insert a second number"))

def gcf (number1, number2):
   if number1 > number2:
      number1, number2 = number2, number1
   for i in range(number1, 0, -1):
      if number1%i == 0 and number2%i == 0:
         return i

print(gcf(number1, number2))