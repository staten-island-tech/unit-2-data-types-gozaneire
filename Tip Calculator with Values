values = [1,2.23,5,7,2,30,15]
print(values[0])
print(values[6])
for i in values:
    print(i)

number = int(input("Pick a Number"))
if number%2 == 0:
    print("even")
else:
    print("odd")

print("What is your bill")
tip = (input("How was the service?"))
if tip == "great":
    [print(number*float(1.25))]
elif tip == "good":
    [print(number*float(1.2))]
elif tip == "okay":
    [print(number*float(1.15))]
elif tip == "bad":
    [print(number)]
